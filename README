# SU KHAC BIỆT GIỮA OBJECT ASSGIN VÀ ...
var root = { a: 1 };
// Tao object cung 1 dia chi vung nho voi root
var a = Object.assign(root);
// Tạo ra một object mới không trùn dia chi vung nho voi bien a 
var b = Object.assign({}, root);
// Clone object es6 
var d = {...root};
console.log(a === root);
console.log(a === b);
console.log(a === d);


